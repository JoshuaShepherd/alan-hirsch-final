{
  "name": "alan-hirsch-digital-platform",
  "private": true,
  "packageManager": "pnpm@10.18.0",
  "scripts": {
    "dev": "turbo dev --filter=safety",
    "build": "turbo build",
    "build:app": "next build",
    "start": "next start",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "turbo type-check",
    "verify": "pnpm type-check && pnpm lint && pnpm format:check",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "clean": "turbo clean",
    "packages:build": "turbo build --filter='@platform/*'",
    "packages:dev": "turbo dev --filter='@platform/*'",
    "db:setup": "npx tsx lib/db/setup.ts",
    "db:seed": "npx tsx lib/db/seed.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:reset": "tsx lib/db/reset.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:fast": "vitest run __tests__/simple.test.ts __tests__/basic-ui.test.tsx __tests__/api/basic-api.test.ts __tests__/ui/basic-component.test.tsx __tests__/utils/test-data-factories.test.ts __tests__/unit/math-utils.test.ts __tests__/integration/database-mock.test.ts --reporter=basic --no-coverage",
    "test:parallel": "vitest run --threads --maxConcurrency=4",
    "test:unit": "vitest run __tests__/unit/ --reporter=basic --no-coverage",
    "test:integration": "vitest run __tests__/integration/ --threads --testTimeout=15000",
    "test:api": "vitest run __tests__/api/ --testTimeout=10000 --threads",
    "test:ui:fast": "vitest run __tests__/ui/ --environment=jsdom --reporter=basic",
    "test:utils": "vitest run __tests__/utils/ --no-coverage --reporter=basic",
    "test:ci": "vitest run --coverage --reporter=json --outputFile=test-results/results.json",
    "test:watch:fast": "vitest --watch --reporter=basic --no-coverage",
    "test:performance": "tsx scripts/test-performance-monitor.ts",
    "test:optimize": "tsx scripts/test-pipeline-optimizer.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:registration": "playwright test tests/e2e/registration-journey.spec.ts",
    "test:e2e:assessments": "playwright test tests/e2e/assessment-journey.spec.ts",
    "test:e2e:content": "playwright test tests/e2e/content-journey.spec.ts",
    "test:phase6": "tsx scripts/phase6-e2e-test-runner.ts",
    "test:deployment:build": "vitest run __tests__/deployment/build-process.test.ts",
    "test:deployment:pipeline": "vitest run __tests__/deployment/pipeline.test.ts",
    "test:deployment:production": "vitest run __tests__/deployment/production-readiness.test.ts",
    "test:phase9": "tsx scripts/phase9-deployment-test-runner.ts",
    "test:all": "pnpm test && pnpm test:e2e",
    "test:ui:pages": "vitest run __tests__/ui/pages-simple.test.tsx",
    "test:ui:forms": "vitest run __tests__/ui/forms-simple.test.tsx",
    "test:ui:components": "vitest run __tests__/ui/components-simple.test.tsx",
    "test:ui:all": "vitest run __tests__/ui/*-simple.test.tsx",
    "test:phase5": "tsx scripts/phase5-ui-test-runner.ts",
    "health-check": "tsx scripts/health-check.ts",
    "weekly-audit": "tsx scripts/weekly-audit.ts",
    "validate:schemas": "tsx scripts/validate-schema-alignment.ts",
    "validate:mappers": "tsx scripts/validate-mapper-alignment.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
