# Supabase project
SUPABASE_PROJECT_ID=your-project-id
SUPABASE_URL=https://your-project-id.supabase.co

# Public (client) key â€” safe to expose
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-public-key

# Service role (server-only)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database password
DB_PASSWORD=your-db-password
PGPASSWORD=your-db-password

# Direct Postgres connection
DIRECT_DATABASE_URL="postgresql://postgres:your-db-password@db.your-project-id.supabase.co:5432/postgres"

# Transaction pooler connection
TRANSACTION_POOLER_DATABASE_URL="postgresql://postgres.your-project-id:your-db-password@aws-1-us-east-1.pooler.supabase.com:6543/postgres"

# Session pooler connection
SESSION_POOLER_DATABASE_URL="postgresql://postgres.your-project-id:your-db-password@aws-1-us-east-1.pooler.supabase.com:5432/postgres"

# Generic DATABASE_URL
POSTGRES_URL="postgresql://postgres.your-project-id:your-db-password@aws-1-us-east-1.pooler.supabase.com:6543/postgres"

# Optional PG_* envs
PGHOST=db.your-project-id.supabase.co
PGPORT=5432
PGUSER=postgres
PGDATABASE=postgres
PGPASSWORD=your-db-password

# Convenience duplicate vars
SUPABASE_ANON_KEY=your-anon-public-key